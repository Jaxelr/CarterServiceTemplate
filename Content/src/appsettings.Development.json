{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AppSettings": {
    "Cache": {
      "CacheTimespan": 60,
      "CacheMaxSize": 2048,
      "CacheEnabled": false
    },
    "RouteDefinition": {
      "RoutePrefix": "openapi/ui",
      "SwaggerEndpoint": "/openapi"
    },
    "ServerUrls": [ "http://localhost:5000" ]
  }
}
