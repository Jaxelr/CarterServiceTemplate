{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AppSettings": {
    "Cache": {
      "CacheTimespan": 60,
      "CacheMaxSize": 2048,
      "CacheEnabled": false
    },
    "RouteDefinition": {
      "RouteSuffix": "swagger"
    },
    "HealthDefinition": {
      "Endpoint": "/health",
      "Name": "Heartbeat",
      "HealthyMessage": "Service is operational",
      "Tags": [ "Systems" ]
    },
    "ServerUrls": [ "http://localhost:5000" ]
  }
}
